{
  "name": "@marklogic-community/grove-core-react-components",
  "version": "1.0.0",
  "description": "React component library for apps backed by MarkLogic",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npm run lint && nwb build-react-component --copy-files",
    "buildStorybook": "build-storybook -c .storybook -o .storybook/dist",
    "clean": "nwb clean-module && nwb clean-demo",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "nwb serve-react-demo",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "npm run lint && nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "npm run lint && nwb test-react --server"
  },
  "dependencies": {
